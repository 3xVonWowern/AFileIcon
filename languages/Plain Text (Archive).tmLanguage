<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>name</key>
    <string>Plain Text (Archive)</string>
    <key>fileTypes</key>
    <array>
      <string>.zip</string>
      <string>.7z</string>
      <string>.rar</string>
      <string>.7zip</string>
      <string>.tgz</string>
      <string>.gz</string>
      <string>.pzip</string>
      <string>.tar</string>
      <string>.wim</string>
    </array>
    <key>patterns</key>
    <array>
      <dict>
        <key>include</key>
        <string>text.plain</string>
      </dict>
    </array>
    <key>scopeName</key>
    <string>text.plain, text.plain.archive</string>
  </dict>
</plist>
